{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app/commons/(.*)": "<rootDir>/../libs/commons/src/$1",
    "@app/commons": "<rootDir>/../libs/commons/src",
    "@app/execute/(.*)": "<rootDir>/../libs/execute/src/$1",
    "@app/execute": "<rootDir>/../libs/execute/src",
    "@app/scenarioSnapshot/(.*)": "<rootDir>/../libs/scenarioSnapshot/src/$1",
    "@app/scenarioSnapshot": "<rootDir>/../libs/scenarioSnapshot/src",
    "@app/schemas/(.*)": "<rootDir>/../libs/schemas/src/$1",
    "@app/schemas": "<rootDir>/../libs/schemas/src",
    "@app/strategy/(.*)": "<rootDir>/../libs/strategy/src/$1",
    "@app/strategy": "<rootDir>/../libs/strategy/src",
    "@app/check/(.*)": "<rootDir>/../libs/check/src/$1",
    "@app/check": "<rootDir>/../libs/check/src"
  }
}